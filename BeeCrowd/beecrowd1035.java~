import java.util.Scanner;
public class beecrowd1035{
  public static void main(String args[]){
    Scanner sc= new Scanner(System.in);
    int a=sc.nextInt();
    int b=sc.nextInt();
    int c=sc.nextInt();
    int d=sc.nextInt();
    boolean flag1 = false;
    boolean flag2 = false;
    boolean flag3 = false;
    boolean flag4 = false;
    if(b>c && a>c){
      flag1=true;
    }
    if(c+d>a+b){
      flag2=true;
    }
      if(c>0 && d>0){
        flag3=true;
    }
      if(a%2==0){
        flag4=true;
    }
      if(flag1 && flag2 && flag3 && flag4){
        System.out.println("Valores aceitos");
      }
      else{
        System.out.println("Valores nao aceitos");
      }
  }
}
      